# AntyBoty #

on connect:
    if {gracz::%player%} is set:
        stop
    kick player due to "&6[ &cAntyBot &6] &7Aby zagrac wbij ponownie na serwer."
    set {gracz::%player%} to now
#Reload serwera#

command /reload:
	trigger:
		send "{@tag} &cPrzeladowanie serwera za 5 sekund."
		wait 5 second
		make player execute command "sk reload all"
		wait 10 second
		send "{@tag} &9Pomyslnie przeladowano caly serwer"

# command of kick players #

command /kick [<player>] [<text>]:
        trigger:
                if arg 1 is set:
                        if arg 2 is set:
                                if player has permission "Ntools.kick":
                                        if argument 1 has permission "essentialsskript.kick":
                                                send "{@tag} &cNie mozesz wyrzucic tego gracza!"
                                                stop
                                        else:
                                                kick the argument 1 due to "%arg 2%"
                                                broadcast "{@tag} &9Gracz &c%arg 1% &9zostal wyrzucony za &c%arg 2%"
                                                stop
                                else:
                                        send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.kick)}"
                                        stop
                        else:
                                send "{@tag} &cPodaj powod!"
                                stop
                else:
                        send "{@tag} &cPodaj nick gracza!"
                        stop
						
# Heal and feed #

command /heal [<player>]:
        aliases: ulecz
        trigger:
                if arg 1 is not set:
                        if player's health is not 10:
                                if player has permission "Ntools.heal":
                                        set the player's food level to 10
                                        heal the player by 10 hearts
                                        send "{@tag} &aUleczono!"
                                        stop
                                else:
                                        send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.heal)"
                                        stop
                        else:
                                send "{@tag} &cTwoje zycie jest pelne!"
                                stop
                if arg 1 is set:
                        if argument's health is not 10:
                                if player has permission "Ntools.heal":
                                        set the argument's food level to 10
                                        heal the argument by 10 hearts
                                        send "{@tag} &bUleczono!" to arg 1
                                        send "{@tag} &bUleczono gracza &c%arg 1%"
                                        stop
                                else:
                                        send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.heal)"
                                        stop
                        else:
                                send "{@tag} &cZycie tego gracza jest pelne!"
                                stop
 
                               
command /feed [<player>]:
        aliases: nakarm
        trigger:
                if arg 1 is not set:
                        if player's food level is not 10:
                                if player has permission "Ntools.feed":
                                        set the player's food level to 10
                                        send "{@tag} &bNakarmiono!"
                                        stop
                                else:
                                        send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.feed)"
                                        stop
                        else:
                                send "{@tag} &cTwoj glod jest pelny!"
                                stop
                if arg 1 is set:
                        if argument's food level is not 10:
                                if player has permission "Ntools.feed":
                                        set the argument's food level to 10
                                        send "{@tag} &bNakarmiono!" to arg 1
                                        send "{@tag} &bNakarmiono gracza &c%arg 1%"
                                        stop
                                else:
                                        send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.feed)"
                                        stop
                        else:
                                send "{@tag} &cGlod tego gracza jest pelny!"
                                stop
                               
# hat #
 
command /hat <material>:
        aliases: czapka
        trigger:
                if player has permission "Ntools.hat":
                        if arg 1 is set:
                                if id of argument 1 is greater than 255:
                                        send "{@tag} &cNie mozesz zalozyc tego itemku tylko bloki!"
                                        stop
                                else:
                                        if helmet slot of the player is not empty:
                                                send "{@tag} &cNajpierw zdejmij przedmiot z glowy!"
                                                stop
                                        else:
                                                set helmet of the player to argument
                                                send "{@tag} &bCiesz sie nowym kapeluszem!"
                                                stop
                        else:
                                send "{@tag} &cWybierz id bloku!"
                                stop
                else:
                        send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.hat)"
                        stop

# Nie Dotykaj bo nie bd dzialac caly sk 0o0#

on skript start:
        send "&8Serwer pracuje na wersji: %skript version%" to the console
        send "" to the console
        send "&8--------------------------------------------------------------" to the console
        send "&8|                                                            |" to the console
        send "&8|                  Enable &9Ntools                         &8|" to the console
        send "&8|                                                            |" to the console
        send "&8--------------------------------------------------------------" to the console
        send "" to the console
        send "&8Autor: &aNaciukany" to the console
        send "&8Version: &71" to the console
        send "" to the console
		
# vanish #

command /vanish [<text>]:
	permission message: &4Blad: &cNie posiadasz uprawnienia &8(Ntools.vanish)
	permission: Ntools.vanish"
	trigger:
		if {Vanish::%player%} is true:
			send "{@tag}&f&lWylaczyles Niewidzialnosc."
			reveal player to all players
			set tag of player to "&f"
			set {Vanish::%player%} to false
		else:
			set {Vanish::%player%} to true
			send "{@tag} &f&lWlaczyles Niewidzialnosc."
			loop all players:
				if loop-player doesn't have permission "Ntools.vanish".see":
					hide player for loop-player
			set tag of player to "&4[NieWidacGo] &f"
on join:
	reveal player to all players
	set {Vanish::%player%} to false
	set tag of player to "&f"
on join:
	wait 1 tick
	loop all players:
		if loop-player has permission "Ntools.vanish":
			reveal loop-player to player
			
# Drop #

on mine of stone:
        add 1 to {scoreboard::stone::%player%}
        chance of {@diamenty}%:
                give {@Mnoznik} diamond to player
                add 1 to {scoreboard::diaxy::%player%}
                send "{@tag} &7Gratulacje trafiles na: &b&lDiament! &7[ x{@Mnoznik} ]" 
                               
        chance of {@zloto}%:
                give {@Mnoznik} gold ingot to player
                add 1 to {scoreboard::zloto::%player%}
                send "{@tag} &7Gratulacje trafiles na: &e&lZloto! &7[ x{@Mnoznik} ]" 
   
        chance of {@zelazo}%:
                give {@Mnoznik} iron ingot to player
                send "{@tag} &7Gratulacje trafiles na: &f&lZelazo! &7 [ x{@Mnoznik} ]" 
   
        chance of {@emerady}%:
                give {@Mnoznik} emerald to player
                send "{@tag} &7Gratulacje trafiles na: &a&lEmeralda ! &7[ x{@Mnoznik} ]" 
         
        chance of {@obsydian}%:
                give {@Mnoznik} obsidian to player
                send "{@tag} &7Gratulacje trafiles na: &1&lObsydian ! &7[ x{@Mnoznik} ]" 
         
        chance of {@wungiel}%:
                give {@Mnoznik} coal to player
                send "{@tag} &7Gratulacje trafiles na: &0&lWegiel ! &7[ x{@Mnoznik} ]" 
           
        chance of {@gunpowder}%:
                give {@Mnoznik} gunpowder to player
                send "{@tag} &7Gratulacje trafiles na: &8&lProszek ! &7[ x{@Mnoznik} ]" 
         
        chance of {@jablko}%:
                give {@Mnoznik} apple to player
                send "{@tag} &7Gratulacje trafiles na: &4&lJablko ! &7[ x{@Mnoznik} ]" 
 
        chance of {@piasek}%:
                give {@Mnoznik} sand to player
                send "{@tag} &7Gratulacje trafiles na: &e&lPiasek ! &7[ x{@Mnoznik} ]" 
     
        chance of {@perelka}%:
                give {@Mnoznik} ender pearl to player
                send "{@tag} &7Gratulacje trafiles na: &c&lPerelke ! &7[ x{@Mnoznik} ]" 
 
        chance of {@book}%:
                give {@Mnoznik} Book to player
                send "{@tag} &7Gratulacje trafiles na: &9&lKsiazke ! &7[ x{@Mnoznik} ]" 
#
command /drop:
	trigger:
		open chest with 4 rows named "&7Drop Uzytkownika: &6%player%" to player
		wait 2 ticks
		format slot 0 of player with 1 of diamond named "&e&lDiamenty" with lore "&7Procent &c{@diamenty}" to close
		format slot 1 of player with 1 of gold ingot named "&e&lZloto" with lore "&7Procent &c{@zloto}" to close
		format slot 2 of player with 1 of Iron Ingot named "&e&lZelazo" with lore "&7Procent &c{@zelazo}" to close
		format slot 3 of player with 1 of emerald named "&e&lSzmaragdy" with lore "&7Procent &c{@emerady}" to close
		format slot 4 of player with 1 of obsidian named "&e&lObsydian" with lore "&7Procent &c{@obsydian}" to close
		format slot 5 of player with 1 of coal named "&e&lWegiel" with lore "&7Procent &c{@wungiel}" to close
		format slot 6 of player with 1 of gunpowder named "&e&lProszek" with lore "&7Procent &c{@gunpowder}" to close
		format slot 7 of player with 1 of apple named "&e&lJablko" with lore "&7Procent &c{@jablko}" to close
		format slot 8 of player with 1 of sand named "&e&lPiasek" with lore "&7Procent &c{@piasek}" to close
		format slot 9 of player with 1 of ender pearl named "&e&lPerly" with lore "&7Procent &c{@perelka}" to close
		format slot 10 of player with 1 of Book named "&e&lKsiazki" with lore "&7Procent &c{@book}." to close
		format slot 22 of player with {@Mnoznik} of Bedrock named "&c&l>> Drop x&4 {@Mnoznik}" with lore "&7Ilosc wypadanych itemow" to close
		
# Stoniarki#

on script load:
    register new shaped recipe for emerald ore named "&aMagiczna stoniarka" using stone, stone, stone, stone, emerald, stone, stone, stone, stone
on place of emerald ore:
    if name of the player's tool is "&aMagiczna stoniarka":
        send "&7Postawiles: &aMagiczna stoniarke"
        set {s.%event-block%} to true
        wait 1 second
        set event-block to stone
        stop
on break of stone:
    if {s.%event-block%} is true:
        if player is holding gold pickaxe:
            cancel event
            send "&7Zniszczyles: &aMagiczna stoniarke"
            set event-block to air
            if player can hold 1 emerald ore named "&aMagiczna stoniarka":
                give 1 emerald ore named "&aMagiczna stoniarka" to the player
                stop
            else:
                drop 1 emerald ore named "&aMagiczna stoniarka"
                stop
        else:
            wait 1 second
            set event-block to stone
            stop

# Sprawdzanie ip graczy oraz poczatki .yml#

on script load:
        if folder "plugins/Ntools/Beta" doesn't exists:
                create folder "plugins/Ntools/Beta"
        if file "plugins/Ntools/Beta/config.yml" doesn't exists:
                create file "plugins/Ntools/Beta/config.yml"
 
on first join:
        set {ip:%player%} to player's ip
        wf "'Adres xD: '%player%' '%{ip:%player%}%'" to "plugins/Ntools/Beta/config.yml"
on join:
        {ip:%player%} is not set:
                set {ip:%player%} to player's ip
                wf "Ntools (BETA): Gracz: %player% Jego Adress IP: %{ip:%player%}%'" to "plugins/Ntools/Beta/config.yml"

# Ustawianie domu #

command /sethome:
	aliases: ustawdom
	trigger:
		if player has permission "Ntools.sethome":
			set {Ntools.%player%.home} to location of player
			send "{@tag} &bUstawiono home!"
			stop
		else:
			send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.sethome)"
			stop
command /home:
	aliases: dom
	trigger:
		if player has permission "Ntools.home":
			if {Ntools.%player%.home} is set:
				if {cooldown} is false:
					teleport player to {Ntools.%player%.home}
					send "{@tag} &aTeleportacja!"
					stop
				else:
					set {_pos} to location of player
					send "{@tag} &7Czekaj &65 sekund&7, nie wolno Ci sie ruszyc!"
					if {_pos} isn't location of player:
						send "{@tag} &cWykryto ruch, teleportacja anulowana!"
						stop
					wait 1 second
					if {_pos} isn't location of player:
						send "{@tag} &cWykryto ruch, teleportacja anulowana!"
						stop
					wait 1 second
					if {_pos} isn't location of player:
						send "{@tag} &cWykryto ruch, teleportacja anulowana!"
						stop
					wait 1 second
					if {_pos} isn't location of player:
						send "{@tag} &cWykryto ruch, teleportacja anulowana!"
						stop
					wait 1 second
					if {_pos} isn't location of player:
						send "{@tag} &cWykryto ruch, teleportacja anulowana!"
						stop
					wait 1 second
					if {_pos} is location of player:
						teleport player to {Ntools.%player%.home}
						send "{@tag} &bTeleportacja..."
						stop
					else:
						send "{@tag} &cWykryto ruch, teleportacja anulowana!"
						stop
			else:
				send "{@tag} &cHome nie zostal ustawiony skontaktuj sie z administratorem!"
				stop
		else:
			send "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.home)"
			stop
command /delhome:
	trigger:
		if player has permission "Ntools.delhome":
			if {Ntools.%player%.home} is set:
				delete {Ntools.%player%.home}
				send "{@tag} &bUsunieto home!"
				stop
			else:
				send "{@tag} &cNie masz domu!"
				stop
		else:
			display "&4Blad: &cNie posiadasz uprawnienia &8(Ntools.delhome)" on bossbar with 100 percent by 1 seconds for player
			stop
# Anty skrypty.#

on damage of player:
	attacker is a player
	victim is a player
	set {_A::%attacker%} to location of attacker
	set {_V::%victim%} to location of victim
	if attacker's tool is bow:
		stop
	else:
		if distance between {_A::%attacker%} and {_V::%victim%} is bigger than 4:
			cancel event
		else:
			stop
	
# Automatyczne Wiadomoscy wysylane do gracza.#

every 60 real seconds:
    {auto.spam} is not set:
        set {auto.spam} to 1
        stop
    {auto.spam} is 1:
        broadcast "{@tag} &7&a&lDziekuje za uzywanie &6&LNtools! :D"
        set {auto.spam} to 2
        stop
    {auto.spam} is 2:
        broadcast "{@tag} Masz propozycje? Wbijaj na &bskype: &7kacper.dawid2"
        set {auto.spam} to 3
        stop
    {auto.spam} is 3:
        broadcast "{@tag} &7Serwer Uzywa Ntools 0.2 by naciukany"
        set {auto.spam} to 4
        stop
    {auto.spam} is 4:
        broadcast "{@tag} &7Wiadomosc przykladowa 2"
        set {auto.spam} to 5
        stop
    {auto.spam} is 5:
        broadcast "{@tag} &7Wiadomosc przykladowa 3"
        set {auto.spam} to 1
        stop

# Ntools - wysylanie pomocy dla gracza #

command /Ntools:
	aliases: naciukany , NaciukTools
	trigger:
		send "&7-------{@tag}&7-------"
		send "&6/G &f-Komendy gildi."
		send "&6/zapisz , /gzapisz &f- Zapis swiata oraz gildi."
		send "&6/helpop &f- Wysyla wiadomosc do administracji."
		send "&6/gm &c<1-2> &f- Ustawia Tryb gry ."
		send "&6/msg &f- Prywatna wiadomosc do innego gracza."
		send "&6/setspawn , /spawn &f- Komendy spawnu."
		send "&6/chat on/off/clear &f- Komendy chatu."
		send "&6/kick <gracz> <powod> &f- Wyrzucanie gracza z serwera."
		send "&6/list &f- Lista graczy na serwerze."
		send "&6/sun /rain &f- Zmiana pogody."
		send "&6/tp /tpall &f- Komendy Teleportacji."
		send "&6/afk &f- Oznajmiasz ze nie ma cie przy komputerze."
		send "&6/fly &f- Komenda latania."
		send "&6/mute /unmute &f- Wyciszanie i odciszanie gracza."
		send "&6/heal &c<grazcz> &6/feed &c<gracz> &f- Karmienie oraz leczenie."
		send "&6/hat &c<blok> &f- Zakladasz blok na glowe."
		send "&6/vanish &f- stajesz sie niewidzialny."
		send "&6/drop &f- Sprawdzasz drop."
		send "&6/home /sethome /delhome &f- Komendy domu."
		send "&6/repair &fNaprawa przedmiotow."
		send "&6/head &f- Otrzymujesz glowe gracza."
		send "&6/god &f- Jestes niesmiertelny."
		send "&6/czas &f- Ustawiasz dzien lub noc."
		send "&6/broadcast &f- Wiadomosc Publiczna do kazdego gracza."
		send "&6/tps &f- liczba tps serwera."
		send "&6/speed &f- Ustawiasz szybkosc"
		send "&6/warp ,/delwarp ,/setwarp &f- warpy."
		send "&6/strzelaj &f- strzelasz mobami."
		
		
# CODE OF GUILDS #
options:
	guild.reqitems: 5 diamond, 1 emerald and 5 stone 
	guild.vip.reqitems: 2 diamond and 2 stone
	guild.item: dragon egg
	guild.aliases: g, guild
	guild.guildradius: 150
	guild.spawnradius: 20
	guild.dolaczitem: 5 diamond
	guild.chat: &8[&f{RANK}&8] {TAG} &7{NICK}&8: &f{MSG}
	guild.tagformat: &8[&2%{_tag}%&8]&f
	teleport.time: 10 
	teleport.type: seconds
	powieksz.item.1: 10 diamond
	powieksz.item.2: 20 diamond
	powieksz.item.3: 30 diamond
	powieksz.item.4: 40 diamond
	powieksz.item.5: 50 diamond
	chat.format.lider: &2[&a{TAG}&2]&a&l [**] &7{NICK}&8: &f{MSG}
	chat.format.normal: &6[&e{TAG}&6]&e&l [*] &7{NICK}&8: &f{MSG}
	sojusz.enabled: true
	potion.shop.waterbreath: 10 emeralds
	potion.shop.fireresistance: 35 emeralds
	potion.shop.invisible: 150 emeralds
	potion.shop.strength: 100 emeralds
	potion.shop.speed: 50 emeralds
	potion.shop.regeneration: 35 emeralds
	potion.shop.nightvision: 15 emeralds
	tab: true

# ====================== KOMENDY ===========================


command /skGuilds:gildia [<text>] [<text>] [<text>] [<text>]:
	aliases: {@guild.aliases}
	trigger:
		if arg 1 is "zaloz" or "create" or "z" or "c":
			if arg 2 is not set:
				send "{@tag} &cWpisz tag gildii."
				stop
			if arg 3 is not set:
				send "{@tag} &cWpisz nazwe gildii."
				stop
			if {skGuilds::inGuild::%player%} is set:
				send "{@tag} &cJestes juz w gildii!"
				stop
			if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is set:
				send "{@tag} &cJestes liderem innej gildii, usun ja aby stworzyc wlasna!"
				stop
			if length of arg 2 is less than 3:
				send "{@tag} &cTag musi miec minimum 3 liter!"
				stop
			if length of arg 2 is greater than 5:
				send "{@tag} &cTag musi miec maximum 5 liter!"
				stop
			if length of arg 3 is greater than 25:
				send "{@tag} &cNazwa nie moze byc dluzsza niz 25 liter!"
				stop
			if length of arg 3 is less than 10:
				send "{@tag} &cNazwa nie moze byc krotsza niz 10 liter!"
				stop
			if {skGuilds::GildiaTag::%arg 2%} is set:
				send "{@tag} &cGildia o takim tagu juz istnieje!"
				stop
			if {skGuilds::GildiaNazwa::%arg 3%} is set:
				send "{@tag} &cGildia o takiej nazwie juz istnieje!"
			if distance between player and {skGuilds::Spawn} is more than 150:
				if player has permission "skguilds.vip":
					if player doesn't have {@guild.vip.reqitems}:
						send "{@tag} &cNie masz wymaganych itemow na gildie. &6(VIP)"
						send "{@tag} &cPotrzebujesz: &6{@guild.vip.reqitems}"
					else: 
						display hologram "&6Gildia &e%arg 3% [%arg 2%]" at location of player for 100 seconds
						remove {@guild.vip.reqitems} from the player
						set {skGuilds::inGuild::%player%} to arg 2
						set {skGuilds::GildiaTag::%arg 2%} to true
						set {skGuilds::GildiaNazwa::%arg 3%} to true
						set {skGuilds::NazwaGildii::%arg 2%} to arg 3
						set {skGuilds::TagGildii::%arg 2%} to arg 2
						set {skGuilds::Zycia::%arg 2%} to 3
						add arg 3 to {skGuilds::Gildie::*}
						add arg 2 to {skGuilds::Tagi::*}
						set {skGuilds::PVP::%arg 2%} to false
						set {skGuilds::%arg 2%::Powieksz::LVL::1} to true
						set {skGuilds::%arg 2%::Powieksz::LVL::2} to true
						set {skGuilds::%arg 2%::Powieksz::LVL::3} to false
						add player to {skGuilds::Czlonkowie::%arg 2%::*}
						set {skGuilds::%arg 2%::Powieksz::LVL::4} to false
						set {skGuilds::%arg 2%::Powieksz::LVL::5} to false
						set {skGuilds::Opis::%arg 2%} to "&eNowa gildia!"
						set {skGuilds::Lider::%arg 2%} to "%player%"
						set {skGuilds::Zabojstwa::%arg 2%} to 0
						set {skGuilds::GuildPoints::%arg 2%} to 1000
						set {_Center} to block at location (x-coordinate of player, y-coordinate of player, z-coordinate of player) in "world"
						set {skGuilds::Baza::%arg 2%} to {_Center}
						set {skGuilds::Polozenie::%arg 2%} to {_Center}
						set block at {_Center} to {@guild.item}
						set {_Gildia} to arg 2
						add {_Gildia} to {skGuilds::Cuboids::*}
						set {skGuilds::Cuboid::Large::%{_Gildia}%} to 70
						set {skGuilds::Cuboid::Center::%{_Gildia}%} to {_Center}
						"%{_Gildia}%".Region.selectLS{{skGuilds::Cuboid::Center::%{_Gildia}%}, {skGuilds::Cuboid::Large::%{_Gildia}%}};
						set {skGuilds::DataZalozenia::%arg 2%} to now
						edit tag of player to "&e[&6%arg 2%&e] &e"
						broadcast "{@tag} Gildia &e%arg 3% &8[&e%arg 2%&8] &7zostala zalozona przez &6[VIP] &e%player%"
						send "{@tag} &6Z racji tego ze jestes VIPem, otrzymujesz na start cuboid o wielkosci 70x70 kratek! Oczywiscie w dalszym stopniu mozesz go powiekszac!"
						stop
				else:
					if player doesn't have {@guild.reqitems}:
						send "{@tag} &cNie masz wymaganych itemow na gildie."
						send "{@tag} &cPotrzebujesz: &7{@guild.reqitems}"
					else: 
						remove {@guild.reqitems} from the player
						set {skGuilds::inGuild::%player%} to arg 2
						set {skGuilds::GildiaTag::%arg 2%} to true
						set {skGuilds::GildiaNazwa::%arg 3%} to true
						set {skGuilds::NazwaGildii::%arg 2%} to arg 3
						set {skGuilds::TagGildii::%arg 2%} to arg 2
						set {skGuilds::Zycia::%arg 2%} to 3
						add arg 3 to {skGuilds::Gildie::*}
						add arg 2 to {skGuilds::Tagi::*}
						set {skGuilds::PVP::%arg 2%} to false
						set {skGuilds::%arg 2%::Powieksz::LVL::1} to false
						set {skGuilds::%arg 2%::Powieksz::LVL::2} to false
						set {skGuilds::%arg 2%::Powieksz::LVL::3} to false
						add player to {skGuilds::Czlonkowie::%arg 2%::*}
						set {skGuilds::%arg 2%::Powieksz::LVL::4} to false
						set {skGuilds::%arg 2%::Powieksz::LVL::5} to false
						set {skGuilds::Opis::%arg 2%} to "&eNowa gildia!"
						set {skGuilds::Lider::%arg 2%} to "%player%"
						set {skGuilds::Zabojstwa::%arg 2%} to 0
						set {skGuilds::GuildPoints::%arg 2%} to 1000
						set {_Center} to block at location (x-coordinate of player, y-coordinate of player, z-coordinate of player) in "world"
						set {skGuilds::Baza::%arg 2%} to {_Center}
						set {skGuilds::Polozenie::%arg 2%} to {_Center}
						set block at {_Center} to {@guild.item}
						set {_Gildia} to arg 2
						add {_Gildia} to {skGuilds::Cuboids::*}
						set {skGuilds::Cuboid::Large::%{_Gildia}%} to 50
						set {skGuilds::Cuboid::Center::%{_Gildia}%} to {_Center}
						"%{_Gildia}%".Region.selectLS{{skGuilds::Cuboid::Center::%{_Gildia}%}, {skGuilds::Cuboid::Large::%{_Gildia}%}};
						set {skGuilds::DataZalozenia::%arg 2%} to now
						edit tag of player to "&e[&6%arg 2%&e] &e"
						broadcast "{@tag} Gildia &e%arg 3% &8[&e%arg 2%&8] &7zostala zalozona przez &e%player%"
						stop
			else:
				send "{@tag} &cJestes za blisko spawnu!"
		if arg 1 is "info" or "informacje" or "i":
			if arg 2 is not set:
				if {skGuilds::inGuild::%player%} is set:
					run player command "/g info %{skGuilds::inGuild::%player%}%" as op
					stop
				else:
					send "{@tag} &cWpisz /g info <TAG>!"
					stop
			else if {skGuilds::GildiaTag::%arg 2%} is not set:
				if arg 2 is set:
					send "{@tag} &cGildia o tagu %arg 2% nie istnieje!"
			else:
				if arg 2 is not set:
					set {_g} to {skGuilds::inGuild::%player%}
					open chest with 1 rows named "&cInformacje o gildii &4%{_g}%" to player
				if arg 2 is set:
					open chest with 1 rows named "&cInformacje o gildii &4%arg 2%" to player
					format slot 8 of player with 1 of glowstone dust named "&6Zycia" with lore "&e%{skGuilds::Zycia::%arg 2%}%" to be unstealable
					format slot 0 of player with 1 of glowstone dust named "&6Opis gildii" with lore "&e%{skGuilds::Opis::%arg 2%}%" to be unstealable
					format slot 1 of player with 1 of glowstone dust named "&6Data zalozenia" with lore "&e%{skGuilds::DataZalozenia::%arg 2%}%" to be unstealable
					format slot 2 of player with 1 of glowstone dust named "&6Ranking" with lore "&e%{skGuilds::GuildPoints::%arg 2%}%" to be unstealable
					format slot 3 of player with 1 of glowstone dust named "&6Wielkosc terenu" with lore "&e%{skGuilds::Cuboid::Large::%arg 2%}%" to be unstealable
					if {skGuilds::Sojusz::%arg 2%::*} is set:
						format slot 4 of player with 1 of glowstone dust named "&6Sojusze" with lore "&e%{skGuilds::Sojusz::%arg 2%::*}%" to be unstealable
					else:
						format slot 4 of player with 1 of gunpowder named "&4Sojusze" with lore "&cBrak!" to be unstealable
					if {skGuilds::PVP::%arg 2%} is false:
						format slot 5 of player with 1 of gunpowder named "&4PVP" with lore "&cWylaczone!" to be unstealable
					else:
						format slot 5 of player with 1 of glowstone dust named "&6PVP" with lore "&aWlaczone!" to be unstealable
					if {skGuilds::Lider::%arg 2%} parsed as player is online:
						format slot 6 of player with 1 of glowstone dust named "&6Lider" with lore "&a%{skGuilds::Lider::%arg 2%}%" to be unstealable
					else:
						format slot 6 of player with 1 of gunpowder named "&4PVP" with lore "&c%{skGuilds::Lider::%arg 2%}%" to be unstealable
					set {_} to size of {skGuilds::Czlonkowie::%arg 2%::*}
					if {_} is 0:
						format slot 7 of player with 1 of gunpowder named "&4Czlonkowie" with lore "&cBrak!" to be unstealable
					else:
						format slot 7 of player with 1 of glowstone dust named "&6Czlonkowie" with lore "&e%{skGuilds::Czlonkowie::%arg 2%::*}%" to be unstealable
						stop
					stop
		if arg 1 is "lista" or "l" or "list":
			if arg 2 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if arg 3 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if {skGuilds::Tagi::*} is not set:
				send "{@tag} &cNa tym serwerze nie ma zadnych gildii!"
				stop
			set {_List::*} to "%{skGuilds::Tagi::*}%"
			set {_num} to 0
			open chest with 6 rows named "&cLista gildii:" to player
			loop {_List::*}:
				format slot {_num} of player with 1 of lapis block named "&e%loop-value%" with lore "&6%{skGuilds::NazwaGildii::%loop-value%}%" to run "xxtajne %loop-value%"
				add 1 to {_num}
				if size of {_List::*} is 0:
					send "{@tag} &cNie ma zadnych aktywnych gildii!"
					stop
			while inventory name of current inventory of player is "&cLista gildii:":
				if size of {_List::*} is not 0:
					loop 53 times:
						set {_} to loop-number
						unformat slot {_}-1 of player
						set {_num} to 0
						loop {_List::*}:
							format slot {_num} of player with 1 of lapis block named "&e%loop-value%" with lore "&6%{skGuilds::NazwaGildii::%loop-value%}%" to run "xxtajne %loop-value%"
							add 1 to {_num}
						wait 1 second

		if arg 1 is "baza" or "dom" or "base" or "b" or "d":
			if arg 2 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if arg 3 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			else if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			if {skGuilds::TPCooldown::%player%} is false:
				set {_pos} to location of player
				set {_index} to {@teleport.time}
				send "{@tag} Zaczekaj &e{@teleport.time} {@teleport.type}&7!"
				set {skGuilds::TPCooldown::%player%} to true
				loop {@teleport.time} times:
					if {_pos} isn't location of player:
						send "{@tag} &cTeleportacja anulowana..."
						stop
					send "&8> &e%{_index}%"
					wait 1 {@teleport.type}
					remove 1 from {_index}
				if {_pos} is location of player:
					set {_g} to {skGuilds::inGuild::%player%}
					teleport player to {skGuilds::Baza::%{_g}%}		
					send "{@tag} Teleportacja..."
					show mob spawner flames at the player
					set {skGuilds::TPCooldown::%player%} to false
					stop
				else:
					send "{@tag} &cTeleportacja anulowana..."
					set {skGuilds::TPCooldown::%player%} to false
					stop	
			else:
				send "{@tag} &cJuz sie teleportujesz!"
				stop
		if arg 1 is "ustawbaze" or "setbase" or "sethome" or "sh" or "sb" or "ub":
			if arg 2 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if arg 3 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			else if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			else if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not player:
				send "{@tag} &cNie jestes liderem gildii!"
			set {_rg} to Region.getAt{location of player};
			if {_rg} is {skGuilds::inGuild::%player%}:
				set {_Polozenie} to location of player
				set {_Gildia} to {skGuilds::inGuild::%player%}
				set {skGuilds::Baza::%{_Gildia}%} to {_Polozenie}
				send "{@tag} Nowa baza gildii zostala ustawiona!"
				stop
			else:
				send "{@tag} &cNie jestes na terenie swojej gildii!"
				stop
		if arg 1 is "usun" or "remove" or "delete" or "u" or "r" or "d" or "del" or "rem" or "rozwiaz":
			if arg 2 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if arg 3 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not player:
				send "{@tag} &cJedynie Lider moze usunac gildie!"
				stop
			set {_arg2} to {skGuilds::inGuild::%player%}
			set {_arg3} to {skGuilds::NazwaGildii::%{_arg2}%}
			edit tag of player to ""
			delete {skGuilds::GildiaTag::%{_arg2}%}
			delete {skGuilds::GildiaNazwa::%{_arg3}%} 
			delete {skGuilds::TagGildii::%{_arg2}%} 
			clear {skGuilds::inGuild::%player%}
			remove {_arg3} from {skGuilds::Gildie::*}
			remove {_arg2} from {skGuilds::Tagi::*}
			delete {skGuilds::Czlonkowie::%{_arg2}%::*}
			delete {skGuilds::PVP::%{_arg2}%}
			delete {skGuilds::%{_arg2}%::Powieksz::LVL::1}
			delete {skGuilds::%{_arg2}%::Powieksz::LVL::2}
			delete {skGuilds::%{_arg2}%::Powieksz::LVL::3}
			delete {skGuilds::%{_arg2}%::Powieksz::LVL::4}
			delete {skGuilds::%{_arg2}%::Powieksz::LVL::5}
			delete {skGuilds::Opis::%{_arg2}%}
			delete {skGuilds::Lider::%{_arg2}%}
			set {skGuilds::GuildPoints::%{_arg2}%} to "Brak!"
			delete {skGuilds::Baza::%{_arg2}%}
			delete {skGuilds::Polozenie::%{_arg2}%} 
			delete {skGuilds::PodbiteGildie::%{_arg2}%} 
			delete {skGuilds::Podbicia::%{_arg2}%} 
			delete {skGuilds::Zabojstwa::%{_arg2}%}
			delete {skGuilds::Zycia::%{_arg2}%}
			set {_Gildia} to {_arg2}
			remove {_Gildia} from {skGuilds::Cuboids::*}
			delete {skGuilds::Cuboid::Large::%{_Gildia}%}
			set block at {skGuilds::Cuboid::Center::%{_Gildia}%} to air
			delete {skGuilds::Cuboid::Center::%{_Gildia}%} 
			delete {skGuilds::DataZalozenia::%{_Gildia}%}
			"%{_arg2}%".Region.delete{};
			broadcast "{@tag} Gildia &e%{_arg3}% &8[&e%{_arg2}%&8] &7zostala rozwiazana przez &e%player%"
			stop
		if arg 1 is "opis" or "desc":
			if arg 2 is set:
				if length of arg 2 is greater than 70:
					send "{@tag} &cDlugosc opisu nie moze byc wieksza niz 70 liter!"
					stop
				if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not player:
					send "{@tag} &cMusisz byc liderem gildii by zmieniac opis!"
					stop
				if arg 3 is set:
					send "{@tag} Nie mozesz podac wiecej niz &e1 argument&7,"
					send "{@tag} ale za to mozesz wpisac &e_ &7poniewaz ten znak"
					send "{@tag} zamieni sie w &espacje&7!"
					stop
				else:
					set {_opis} to "%coloured arg 2%"
					replace all "_" with " " in {_opis}
					set {skGuilds::Opis::%{skGuilds::inGuild::%player%}%} to {_opis}
					send "{@tag} Poprawnie zmieniles opis gildii na: &e%{_opis}%"
		arg 1 is "pvp" or "pv":
			arg 2 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if {skGuilds::inGuild::%player%} is not set:	
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not set:
				send "{@tag} &cNie jestes liderem gildii!"
				stop
			if {skGuilds::PVP::%{skGuilds::inGuild::%player%}%} is true:
				send "{@tag} Ogien sojuszniczy gildii zostal &cwylaczony!"
				set {skGuilds::PVP::%{skGuilds::inGuild::%player%}%} to false
				stop
			else:
				send "{@tag} Ogien sojuszniczy gildii zostal &awlaczony!"
				set {skGuilds::PVP::%{skGuilds::inGuild::%player%}%} to true
				stop
		if arg 1 is "zapros" or "invite" or "inv":
			arg 2 is not set:
				send "{@tag} &cWpisz nick gracza!"
				stop
			if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			set {_player} to arg 2 parsed as player
			if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not player:
				send "{@tag} &cJedynie lider moze zapraszac graczy!"
				stop
			if "%{skGuilds::inGuild::%player%}%" is "%{skGuilds::inGuild::%{_player}%}%":
				send "{@tag} &cTen gracz juz jest w naszej gildii!"
				stop
			if arg 2 is "%player%":
				send "{@tag} &cNie mozesz zaprosic samego siebie!"
				stop
			set {_g} to {skGuilds::inGuild::%player%}
			set {skGuilds::Zaproszenie::%{_player}%::%{skGuilds::inGuild::%player%}%} to true
			send "{@tag} &eZostales zaproszony do gildii &6%{_g}%&e!" to {_player}
			send "{@tag} &eDolacz, za pomoca komendy &6/g dolacz %{_g}%&e!" to {_player}
			send "{@tag} &eKoszt: &6{@guild.dolaczitem}" to {_player}
			send "{@tag} Wyslano zaproszenie do gracza &e%{_player}%&7!"
			wait 1 minute
			if {skGuilds::inGuild::%{_player}%} is not {skGuilds::inGuild::%player%}:
				set {skGuilds::Zaproszenie::%player%::%{skGuilds::inGuild::%player%}%} to false
			stop
		if arg 1 is "dolacz" or "join":
			if player doesn't have {@guild.dolaczitem}:
				send "{@tag} &cNie masz wymaganych przedmiotow!"
				send "{@tag} &cPotrzebujesz: &7{@guild.dolaczitem}"
				stop
			if {skGuilds::inGuild::%player%} is set:
				send "{@tag} &cJestes juz w gildii!"
				stop
			if arg 2 is not set:
				send "{@tag} &cPodaj nazwe gildii!"
				stop

			if {skGuilds::GildiaTag::%arg 2%} is true:
				if {skGuilds::Zaproszenie::%player%::%arg 2%} is true:
					set {skGuilds::inGuild::%player%} to "%arg 2%"
					add "%player%" to {skGuilds::Czlonkowie::%arg 2%::*}
					edit tag of the player to "&6[&e%arg 2%&6] &e"
					set {skGuilds::Zaproszenie::%player%::%arg 2%} to false
					broadcast "{@tag} Gracz &e%player% &7dolaczyl do gildii &e%arg 2%"
					remove {@guild.dolaczitem} from player
					stop
				else:
					send "{@tag} &cNie jestes zaproszony do tej gildii!"
			else:
				send "{@tag} &cTaka gildia nie istnieje!"
		if arg 1 is "opusc" or "leave":
			if arg 3 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is player:
				send "{@tag} &cLider nie moze opuscic gildii!"
				stop
			else:
				broadcast "{@tag} Gracz &e%player% &7opuscil gildie &e%{skGuilds::inGuild::%player%}%"
				remove "%player%" from {skGuilds::Czlonkowie::%{skGuilds::inGuild::%player%}%::*}
				edit tag of player to ""
				clear {skGuilds::inGuild::%player%}
				stop
		if arg 1 is "wyrzuc" or "kick":
			if arg 2 is not set:
				send "{@tag} &cWpisz nick gracza!"
				stop
			if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not player:
				send "{@tag} &cTylko lider gildii moze wyrzucac graczy!"
				stop
			if arg 2 is player:
				send "{@tag} &cNie mozesz wyrzucic siebie z gildii!"
				stop
			if "%{skGuilds::inGuild::%player%}%" is not "%{skGuilds::inGuild::%arg 2%}%":
				send "{@tag} &cGracz &4%arg 2% &cnie jest w twojej gildii!"
				stop
			else:
				broadcast "{@tag} Gracz &e%arg 2% &7zostal wyrzucony z gildii &e%{skGuilds::inGuild::%player%}%"
				edit tag of arg 2 parsed as player to ""
				remove "%arg 2%" from {skGuilds::Czlonkowie::%{skGuilds::inGuild::%player%}%::*}
				clear {skGuilds::inGuild::%arg 2%}
				stop
		if arg 1 is "powieksz" or "bigger":
			arg 2 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			arg 3 is set:
				send "{@tag} &cZa duzo argumentow!"
				stop
			if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not player:
				send "{@tag} &cTylko lider gildii moze powiekszac teren!"
				stop
			set {_gildia} to {skGuilds::inGuild::%player%}
			set {_name} to {skGuilds::inGuild::%player%}
			set {_rg} to Region.getAt{location of player};
			if {_rg} is {skGuilds::inGuild::%player%}:
				if {skGuilds::%{_gildia}%::Powieksz::LVL::1} is false:
					if player doesn't have {@powieksz.item.1}:
						send "{@tag} &cNie masz wymaganych przedmiotow!"
						send "{@tag} &cPotrzebujesz: &7{@powieksz.item.1}"
						stop 
					remove {@powieksz.item.1} from player
					set {skGuilds::Cuboid::Large::%{_Gildia}%} to 60
					"%{_name}%".Region.selectLS{{skGuilds::Cuboid::Center::%{_Gildia}%} , {skGuilds::Cuboid::Large::%{_Gildia}%} }; 
					set {skGuilds::%{_gildia}%::Powieksz::LVL::1} to true
					set {skGuilds::%{_gildia}%::Powieksz::LVL::2} to false
					send "{@tag} &eTeren gildii zostal powiekszony!"
					send "{@tag} &eObecna wielkosc: &660 kratek"
					send "{@tag} &ePoziom: &61"
					stop
				if {skGuilds::%{_gildia}%::Powieksz::LVL::2} is false:
					if player doesn't have {@powieksz.item.2}:
						send "{@tag} &cNie masz wymaganych przedmiotow!"
						send "{@tag} &cPotrzebujesz: &7{@powieksz.item.2}"
						stop 
					remove {@powieksz.item.2} from player
					set {skGuilds::Cuboid::Large::%{_Gildia}%} to 70
					"%{_name}%".Region.selectLS{{skGuilds::Cuboid::Center::%{_Gildia}%} , {skGuilds::Cuboid::Large::%{_Gildia}%} }; 
					set {skGuilds::%{_gildia}%::Powieksz::LVL::2} to true
					set {skGuilds::%{_gildia}%::Powieksz::LVL::3} to false
					send "{@tag} &eTeren gildii zostal powiekszony!"
					send "{@tag} &eObecna wielkosc: &670 kratek"
					send "{@tag} &ePoziom: &62"
					stop
				if {skGuilds::%{_gildia}%::Powieksz::LVL::3} is false:
					if player doesn't have {@powieksz.item.3}:
						send "{@tag} &cNie masz wymaganych przedmiotow!"
						send "{@tag} &cPotrzebujesz: &7{@powieksz.item.3}"
						stop 
					remove {@powieksz.item.3} from player
					set {skGuilds::Cuboid::Large::%{_Gildia}%} to 80
					"%{_name}%".Region.selectLS{{skGuilds::Cuboid::Center::%{_Gildia}%} , {skGuilds::Cuboid::Large::%{_Gildia}%} }; 
					set {skGuilds::%{_gildia}%::Powieksz::LVL::3} to true
					set {skGuilds::%{_gildia}%::Powieksz::LVL::4} to false
					send "{@tag} &eTeren gildii zostal powiekszony!"
					send "{@tag} &eObecna wielkosc: &680 kratek"
					send "{@tag} &ePoziom: &63"
					stop
				if {skGuilds::%{_gildia}%::Powieksz::LVL::4} is false:
					if player doesn't have {@powieksz.item.4}:
						send "{@tag} &cNie masz wymaganych przedmiotow!"
						send "{@tag} &cPotrzebujesz: &7{@powieksz.item.4}"
						stop 
					remove {@powieksz.item.4} from player
					set {skGuilds::Cuboid::Large::%{_Gildia}%} to 90
					"%{_name}%".Region.selectLS{{skGuilds::Cuboid::Center::%{_Gildia}%} , {skGuilds::Cuboid::Large::%{_Gildia}%} }; 
					set {skGuilds::%{_gildia}%::Powieksz::LVL::4} to true
					set {skGuilds::%{_gildia}%::Powieksz::LVL::5} to false
					send "{@tag} &eTeren gildii zostal powiekszony!"
					send "{@tag} &eObecna wielkosc: &690 kratek"
					send "{@tag} &ePoziom: &64"
					stop
				if {skGuilds::%{_gildia}%::Powieksz::LVL::5} is false:
					if player doesn't have {@powieksz.item.5}:
						send "{@tag} &cNie masz wymaganych przedmiotow!"
						send "{@tag} &cPotrzebujesz: &7{@powieksz.item.5}"
						stop 
					remove {@powieksz.item.5} from player
					set {skGuilds::Cuboid::Large::%{_Gildia}%} to 100
					"%{_name}%".Region.selectLS{{skGuilds::Cuboid::Center::%{_Gildia}%} , {skGuilds::Cuboid::Large::%{_Gildia}%} }; 
					set {skGuilds::%{_gildia}%::Powieksz::LVL::5} to true
					send "{@tag} &eTeren gildii zostal powiekszony!"
					send "{@tag} &eObecna wielkosc: &6100 kratek"
					send "{@tag} &ePoziom: &65 (Ostatni)"
					stop
				else:
					send "{@tag} &cOsiagnales maksymalny rozmiar terenu! (p.5) (100 kratek)"
			else:
				send "{@tag} &cMusisz znajdowac sie na terenie gildii by to zrobic!"
				stop
		if arg 1 is "ranking" or "rank":
			if arg 2 is not set:
				send "&8╔═══════ &6%player% &8════════╗"
				send "&8╠&6 Ranking: &e%{skGuilds::Ranking::%player%}%"
				send "&8╠&6 Zabojstwa: &e%{skGuilds::Zabojstwa::%player%}%"
				send "&8╠&6 Zgony: &e%{skGuilds::Zgony::%player%}%"
				if {skGuilds::inGuild::%player%} is not set:
					send "&8╠&6 Gildia: &cBrak!"
					send "&8╠&6 Ostatnie wejscie: &e%{skGuilds::LastJoin::%player%}%"
				else:
					send "&8╠&6 Gildia: &e%{skGuilds::inGuild::%player%}%"
					send "&8╠&6 Ostatnie wejscie: &e%{skGuilds::LastJoin::%player%}%"
				send "&8╚════════════════════╝"
				stop
			if arg 2 is set:
				if arg 2 parsed as player is online:
					send "&8╔═══════ &6%arg 2% &8════════╗"
					send "&8╠&6 Ranking: &e%{skGuilds::Ranking::%arg 2%}%"
					send "&8╠&6 Zabojstwa: &e%{skGuilds::Zabojstwa::%arg 2%}%"
					send "&8╠&6 Zgony: &e%{skGuilds::Zgony::%arg 2%}%"
					if {skGuilds::inGuild::%arg 2%} is not set:
						send "&8╠&6 Gildia: &cBrak!"
						send "&8╠&6 Ostatnie wejscie: &e%{skGuilds::LastJoin::%arg 2%}%"
					else:
						send "&8╠&6 Gildia: &e%{skGuilds::inGuild::%arg 2%}%"
						send "&8╠&6 Ostatnie wejscie: &e%{skGuilds::LastJoin::%arg 2%}%"
					send "&8╚════════════════════╝"
					stop	
				else:
					send "{@tag} &cTen gracz nie jest na serwerze!"
					stop
		if arg 1 is "war" or "wojna" or "bitwa":
			if arg 2 is not set:
				send "{@tag} &cPodaj z jaka gildia chcesz walczyc!"
				stop
			if arg 3 parsed as integer is not set:
				send "{@tag} &cPodaj godzine spotkania!"
				stop
			if arg 4 is not set:
				send "{@tag} &cPodaj dodatkowy tekst!"
				stop
			if {skGuilds::TagGildii::%arg 2%} is not set:
				send "{@tag} &cNie ma takiej gildii!"
				stop
			if {skGuilds::inGuild::%player%} is not set:
				send "{@tag} &cNie jestes w zadnej gildii!"
				stop
			if {skGuilds::War::%arg 2%} is {skGuilds::inGuild::%player%}:
				send "{@tag} &cTa gildia juz walczy z nami!"
				stop
			if {skGuilds::War::%arg 2%} is not set:
				set {skGuilds::War::%arg 2%} to {skGuilds::inGuild::%player%}
				broadcast "{@tag} &eGildia &6%{skGuilds::inGuild::%player%}%&e wyzwala gildie &6%arg 2% &ena wojne!"
				broadcast "{@tag} &eDodatkowe informacje: &6%arg 4%"
				broadcast "{@tag} &eMiejsce: &6%location of player%"
				broadcast "{@tag} &eGodzina: &6%arg 3%:00"
				loop {skGuilds::Czlonkowie::%arg 2%::*}:
					send "{@tag} &4&lTwoja gildia zostala wyzwana na wojne!" to loop-value
					stop
				wait 25 minutes
				delete {skGuilds::War::%arg 2%}
				broadcast "&cBitwa pomiedzy &4%{skGuilds::inGuild::%player%}% &ca &4%arg 2%&c dobiegla konca!"
			else:
				send "{@tag} &cTa gildia juz jest z kims/z nami w walce!"
		if arg 1 is "efekt" or "effect":
			open chest with 3 rows named "&cEfekty:" to player
			wait 1 tick
			format slot 0 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 1 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 2 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 3 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 4 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 5 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 6 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 7 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 8 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 9 of player with 1 of white stained glass pane named "&f" to be unstealable

			format slot 10 of player with 1 of potion of:8237 named "&9Oddychanie pod woda" with lore "&eCena: {@potion.shop.waterbreath}" to run "potionshop %player% 1"
			format slot 11 of player with 1 of potion of:8227 named "&4Ochrona przed ogniem" with lore "&eCena: {@potion.shop.fireresistance}" to run "potionshop %player% 2"
			format slot 12 of player with 1 of potion of:8238 named "&bNiewidzialnosc" with lore "&eCena: {@potion.shop.invisible}" to run "potionshop %player% 3"
			format slot 13 of player with 1 of potion of:8201 named "&4Sila" with lore "&eCena: {@potion.shop.strength}" to run "potionshop %player% 4"
			format slot 14 of player with 1 of potion of:8194 named "&bSzybkosc" with lore "&eCena: {@potion.shop.speed}" to run "potionshop %player% 5"
			format slot 15 of player with 1 of potion of:8193 named "&dRegeneracja Zycia" with lore "&eCena: {@potion.shop.regeneration}" to run "potionshop %player% 6"
			format slot 16 of player with 1 of potion of:8230 named "&1Widzenie w ciemnosci" with lore "&eCena: {@potion.shop.nightvision}" to run "potionshop %player% 7"


			format slot 17 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 18 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 19 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 20 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 21 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 22 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 23 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 24 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 25 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 26 of player with 1 of white stained glass pane named "&f" to be unstealable
			format slot 27 of player with 1 of white stained glass pane named "&f" to be unstealable
			stop

		if arg 1 is "pomoc" or "pomocy" or "helpme":
			if arg 2 is not set:
				send "{@tag} &cMusisz podac z jaka gildia walczysz!"
				stop
			if {skGuilds::Wait::%player%} is not set:
				broadcast "{@tag} &6Gracz &e%player% &6potrzebuje pomocy!"
				broadcast "{@tag} &6Walczy z gildia: &e%arg 2%"
				broadcast "{@tag} &6Jego koordynaty: &e%location of player%"
				set {skGuilds::Wait::%player%} to true
				wait 3 minutes
				delete {skGuilds::Wait::%player%}
				send "{@tag} &cJuz mozesz zglosic sie o pomoc!"
				stop
			else:
				send "{@tag} &cZglaszac pomoc mozna co 3 minuty!" to player
				stop
		if arg 1 is "1":
			send "&8============ {@tag} &8============"
			send "&e/g zaloz &8- &7Zaklada gildie."
			send "&e/g zapros &8- &7Zaprasza do gildii."
			send "&e/g dolacz &8- &7Dolacza do gildii."
			send "&e/g wyrzuc &8- &7Wyrzuca z gildii."
			send "&e/g baza &8- &7Teleportuje do bazy."
			send "&e/g ustawbaze &8- &7Zaklada baze gildii."
			send "&e/g info &8- &7Informacje o gildii."
			send "&e/g ranking &8- &7Ranking graczy."
			send "&8============ 1/2 &8============"
		if arg 1 is "2":
			send "&8============ {@tag} &8============"
			send "&e/g lista &8- &7Lista gildii."
			send "&e/g pvp &8- &7Zmienia ogien sojuszniczy."
			send "&e/g opis &8- &7Zmienia opis gildii."
			send "&e/g usun &8- &7Usuwa gildie."
			send "&e/g powieksz &8- &7Powieksza teren gildii."
			send "&e/guildchat &8- &7Chat gildii."
			send "&e/top10 &8- &7Top 10 graczy"
			send "&e/gtop10 &8- &7Top 10 gildii"
			if player has permission "skguilds.admin" or "skguilds.*":
				send "&4/ga resetuj &8- &cResetuje wszystkie zmienne."
				send "&4/ga ustawspawn &8- &cUstawia globalny spawn."
				send "&4/ga przeladuj &8- &cPrzeladowywuje gildie."
				send "&4/ga zbanuj &8- &cBanuje gildie na zawsze."
				send "&8============ 2/2 &8============"
			else:
				send "&8============ 2/2 &8============"
		if arg 1 is not set:
			send "{@tag} &cNie znaleziono komendy!"
			send "{@tag} &cWpisz &e/g 1/2 &caby dowiedziec sie wiecej!"
			stop

# ====================== GADMIN ========================
command /gadmin [<text>] [<text>] [<text>]:
	aliases: ga, guildadmin, gildiaadmin, gildiadmin
	trigger:
		if player has permission "skguilds.admin" or "skguilds.*":
			if arg 1 is "przeladuj" or "reload":
				send "{@tag} &ePrzeladowywanie pliku..."
				execute console command "/sk reload %script%"
				wait 10 ticks
				send "{@tag} &ePomyslnie przeladowano plik!"
			if arg 1 is "setspawn" or "ustawspawn":
				set {skGuilds::Spawn} to location of player
				send "{@tag} Pomyslnie ustawiles globalny spawn gildii!"
				stop
			if arg 1 is "clear" or "czysc" or "resetuj" or "reset":
				if arg 2 is set:
					send "{@tag} &cZa duzo argumentow!"	
				if arg 3 is set:
					send "{@tag} &cZa duzo argumentow!"	
				else:
					send "{@tag} Resetowanie &cwszystkich &7danych... zaczekaj chwile."
					wait 2 seconds
					clear {skGuilds::*}
					delete {skGuilds::*}
					kick all players due to "&4[skGuilds] &cResetowanie wszystkich danych skryptu..."
					stop
			if arg 1 is "usun" or "rozwiaz" or "zbanuj":
				if arg 2 is not set:
					send "{@tag} &cMusisz podac gildie!"
					stop
				if arg 3 is not set:
					send "{@tag} &cMusisz podac powod!"
					stop
				if {skGuilds::GildiaTag::%arg 2%} is not set:
					send "{@tag} &cNie ma takiej gildii!"
					stop
				if arg 2 is set:
					if arg 3 is set:
						set {_l} to {skGuilds::Lider::%arg 2%}
						set {_arg2} to arg 2
						set {_arg3} to {skGuilds::NazwaGildii::%{_arg2}%}
						edit tag of {_l} to ""
						delete {skGuilds::GildiaTag::%{_arg2}%}
						delete {skGuilds::GildiaNazwa::%{_arg3}%} 
						delete {skGuilds::TagGildii::%{_arg2}%} 
						clear {skGuilds::inGuild::%{_l}%}
						remove {_arg3} from {skGuilds::Gildie::*}
						remove {_arg2} from {skGuilds::Tagi::*}
						delete {skGuilds::Czlonkowie::%{_arg2}%::*}
						delete {skGuilds::PVP::%{_arg2}%}
						delete {skGuilds::%{_arg2}%::Powieksz::LVL::1}
						delete {skGuilds::%{_arg2}%::Powieksz::LVL::2}
						delete {skGuilds::%{_arg2}%::Powieksz::LVL::3}
						delete {skGuilds::%{_arg2}%::Powieksz::LVL::4}
						delete {skGuilds::%{_arg2}%::Powieksz::LVL::5}
						delete {skGuilds::Opis::%{_arg2}%}
						delete {skGuilds::Lider::%{_arg2}%}
						delete {skGuilds::Zycia::%{_arg2}%}
						set {skGuilds::GuildPoints::%{_arg2}%} to "Brak!"
						delete {skGuilds::Baza::%{_arg2}%}
						delete {skGuilds::Polozenie::%{_arg2}%} 
						delete {skGuilds::PodbiteGildie::%{_arg2}%} 
						delete {skGuilds::Podbicia::%{_arg2}%} 
						delete {skGuilds::Zabojstwa::%{_arg2}%}
						set {_Gildia} to {_arg2}
						remove {_Gildia} from {skGuilds::Cuboids::*}
						delete {skGuilds::Cuboid::Large::%{_Gildia}%}
						set block at {skGuilds::Cuboid::Center::%{_Gildia}%} to air
						delete {skGuilds::Cuboid::Center::%{_Gildia}%} 
						delete {skGuilds::DataZalozenia::%{_Gildia}%}
						"%{_arg2}%".Region.delete{};
						broadcast ""
						broadcast "&4Gildia %arg 2% zostala zbanowana!"
						broadcast "&4Powod: &c%arg 3%"
						broadcast ""
						send "{@tag} &cPoprawnie zbanowales/usunales gildie!"
		else:
			send "{@tag} &cNie masz do tego uprawnien!"


# ====================== SOJUSZ ===========================
command /sojusz [<text>] [<text>]:
	trigger:
		if {skGuilds::inGuild::%player%} is set:
			if arg 1 is not set:
				send "&8============ {@tag} &8============"
				send "&e/sojusz zapros &8- &7Wysyla zaproszenie do sojuszu"
				send "&e/sojusz akceptuj &8- &7Akceptuje zaproszenie do sojuszu"
				send "&e/sojusz zerwij &8- &7Zrywa sojusz z gildia"
				send "&8============ {@tag} &8============"
				stop
			if arg 1 is "zapros" or "invite" or "inv":
				if arg 2 is not set:
					send "{@tag} &cMusisz podac tag gildii!"
					stop
				if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not set:
					send "{@tag} &cNie jestes liderem zadnej gildii!"
					stop
				if {skGuilds::GildiaTag::%arg 2%} is not set:
					send "{@tag} &cNie ma takiej gildii!"
					stop
				if {skGuilds::ZDS::%arg 2%} is true:
					send "{@tag} &cTa gildia jest juz zaproszona do sojuszu!"
					stop
				if {skGuilds::Sojusz::%{skGuilds::inGuild::%player%}%} is "%arg 2%":
					send "{@tag} &cTa gildia juz jest z wami w sojuszu!"
					stop
				if {skGuilds::Lider::%arg 2%} parsed as player is offline:
					send "{@tag} &cLider gildii %arg 2% nie jest online!"
					stop
				if arg 2 is {skGuilds::inGuild::%player%}:
					send "{@tag} &cNie mozesz zawrzec sojuszu z wlasna gildia!"
					stop
				if {skGuilds::inGuild::%player%} is not set:
					send "{@tag} &cNie jestes w zadnej gildii!"
					stop
				set {_g} to {skGuilds::inGuild::%player%}
				set {_lider} to {skGuilds::Lider::%arg 2%} parsed as player
				send "{@tag} Wyslales zaproszenie do sojuszu, dla gildii &e%arg 2%"
				send "{@tag} &eTwoja gildia zostala zaproszona do sojuszu!" to {_lider}
				send "{@tag} &eAkceptuj komenda &6/sojusz akceptuj %{_g}%&e!" to {_lider}
				set {skGuilds::ZDS::%arg 2%} to true
				wait 120 second
				set {skGuilds::ZDS::%arg 2%} to false
				stop
			if arg 1 is "akceptuj":
				if arg 2 is not set:
					send "{@tag} &cMusisz podac tag gildii!"
					stop
				if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not set:
					send "{@tag} &cNie jestes liderem zadnej gildii!"
					stop
				if {skGuilds::GildiaTag::%arg 2%} is not set:
					send "{@tag} &cNie ma takiej gildii!"
					stop
				if {skGuilds::ZDS::%arg 2%} is false:
					send "{@tag} &cNie masz zaproszenia od tej gildii!"
					stop
				if {skGuilds::Sojusz::%{skGuilds::inGuild::%player%}%} is "%arg 2%":
					send "{@tag} &cTa gildia jest juz z wami w sojuszu!"
					stop
				set {_nick} to {skGuilds::Lider::%arg 2%} parsed as player
				set {_g} to {skGuilds::inGuild::%player%}
				set {skGuilds::Sojusz::%{_g}%} to "%arg 2%"
				delete {skGuilds::ZDS::%arg 2%} 
				add "%arg 2%" to {skGuilds::Sojusz::%{_g}%::*}
				add "%{_g}%" to {skGuilds::Sojusz::%arg 2%::*}
				broadcast "{@tag} &7Gildia &e%{_g}% &7zawarla sojusz z gildia &e%arg 2%"
			if arg 1 is "zerwij":
				if arg 2 is not set:
					send "{@tag} &cMusisz podac tag gildii!"
					stop
				if {skGuilds::Lider::%{skGuilds::inGuild::%player%}%} is not set:
					send "{@tag} &cNie jestes liderem zadnej gildii!"
					stop
				if {skGuilds::GildiaTag::%arg 2%} is not set:
					send "{@tag} &cTaka gildia nie istnieje!"
					stop
				if {skGuilds::Sojusz::%arg 2%} is not set:
					send "{@tag} &cNie jestes w sojuszu z ta gildia!"
					stop
				set {_nick} to {skGuilds::Lider::%arg 2%} parsed as player
				delete {skGuilds::Sojusz::%arg 2%}
				remove "%arg 2%" from {skGuilds::Sojusz::%{_g}%::*}
				remove "%{_tag}%" from {skGuilds::Sojusz::%arg 2%::*}
				broadcast "{@tag} Gildia &e%{skGuilds::inGuild::%player%}% &7zerwala sojusz z gildia &e%arg 2%"
		else:
			send "{@tag} &cNie masz zadnej gildii!"

# ====================== CHAT GILDYJNY ===========================
command /guildchat [<text>]:
	aliases: c, chat, gchat, gc
	trigger:
		if arg 1 is not set:
			send "{@tag} &cMusisz podac wiadomosc!"
			stop
		if {skGuilds::inGuild::%player%} is set:	
			set {_g} to {skGuilds::inGuild::%player%}
			if {skGuilds::Lider::%{_g}%} is player:
				loop {skGuilds::Czlonkowie::%{_g}%::*}:
					loop all players:
						loop-value is loop-player:
							set {_guildchatlider} to {guildchat.lider}
							replace all "{TAG}" with "%{skGuilds::inGuild::%player%}%" in {_guildchatlider}
							replace all "{NICK}" with "%player%" in {_guildchatlider}
							replace all "{MSG}" with "%arg 1%" in {_guildchatlider}
							send "%{_guildchatlider}%" to loop-player
							stop
			else:
				loop {skGuilds::Czlonkowie::%{_g}%::*}:
					loop all players:
						loop-value is loop-player:
							set {_guildchatnormal} to {guildchat.normal}
							replace all "{TAG}" with "%{skGuilds::inGuild::%player%}%" in {_guildchatnormal}
							replace all "{NICK}" with "%player%" in {_guildchatnormal}
							replace all "{MSG}" with "%arg 1%" in {_guildchatnormal}
							send "%{_guildchatnormal}%" to loop-player
							stop
		else:
			send "{@tag} &cNie jestes w zadnej gildii!"
			stop
on load:
	set {guildchat.lider} to "{@chat.format.lider}"
	set {guildchat.normal} to "{@chat.format.normal}"

# ====================== TOP 10 - Pomógł PiratJaskiniowy. Dzięki! ===========================


command /top10:
    trigger:
        send "{@tag} &8{ &cTOP 10 GRACZY &8}"
        loop {skGuilds::Ranking::*}:
            set {_lista::%loop-index%} to loop-value
        set {_num} to size of {_lista::*}
        loop {_num} times:
            loop {_lista::*}:
                {_value.%loop-number%} is not set:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
                loop-value is more than {_value.%loop-number%}:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
            delete {_lista::%{_index.%loop-number%}%}
        loop 10 times:
            if loop-number is less than or equal to {_num}:
                send "&6%loop-number%. &f%{skGuilds::Nick::%{_index.%loop-number%}%}% &8(&e%{_value.%loop-number%}%&8)"


command /gtop10:
    trigger:
        send "{@tag} &8{ &cTOP 10 GILDII &8}"
        loop {skGuilds::GuildPoints::*}:
            set {_lista::%loop-index%} to loop-value
        set {_num} to size of {_lista::*}
        loop {_num} times:
            loop {_lista::*}:
                {_value.%loop-number%} is not set:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
                loop-value is more than {_value.%loop-number%}:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
            delete {_lista::%{_index.%loop-number%}%}
        loop 10 times:
            if loop-number is less than or equal to {_num}:
                send "&6%loop-number%. &f%{skGuilds::TagGildii::%{_index.%loop-number%}%}% &8(&e%{_value.%loop-number%}%&8)"

every 1 second:
    if {@tab} is true:
        loop {skGuilds::GuildPoints::*}:
            set {_lista::%loop-index%} to loop-value
        set {_num} to size of {_lista::*}
        loop {_num} times:
            loop {_lista::*}:
                {_value.%loop-number%} is not set:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
                loop-value is more than {_value.%loop-number%}:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
            delete {_lista::%{_index.%loop-number%}%}
        loop 10 times:
            if loop-number is less than or equal to {_num}:
                set {g.tab.%loop-number%} to "&6%loop-number%. &f%{skGuilds::TagGildii::%{_index.%loop-number%}%}%"
            else:
                set {g.tab.%loop-number%} to "&6%loop-number%. "
every 1 second:
    if {@tab} is true:
        loop {skGuilds::Ranking::*}:
            set {_lista::%loop-index%} to loop-value
        set {_num} to size of {_lista::*}
        loop {_num} times:
            loop {_lista::*}:
                {_value.%loop-number%} is not set:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
                loop-value is more than {_value.%loop-number%}:
                    set {_value.%loop-number%} to loop-value
                    set {_index.%loop-number%} to loop-index
            delete {_lista::%{_index.%loop-number%}%}
        loop 10 times:
            if loop-number is less than or equal to {_num}:
                set {p.tab.%loop-number%} to "&6%loop-number%. &f%{skGuilds::Nick::%{_index.%loop-number%}%}%"
            else:
                set {p.tab.%loop-number%} to "&6%loop-number%. "

# ====================== TAB ===========================
